{
  "@context": {
    "@vocab": "http://schema.org"
  },
  "meta": {
    "owners": [],
    "name": "Fluency Detection",
    "description": "Fluency detection on Twitter communication networks.",
    "domain": "Computational Linguistics",
    "id_property": "id",
    "entity_type_property": "entity_type"
  },
  "entity_types": {
    "tweet": {
      "meta": {
        "display": "{0.text}"
      },
      "properties": [
        "text",
        "twitter_language"
      ]
    },
    "user": {
      "meta": {
        "display": "{0.id}"
      },
      "properties": [
        "is_bot",
        "fluent_in_eng",
        "fluent_in_ukr",
        "fluent_in_rus",
        "fluent_in_und"
      ]
    }
  },
  "properties": {
    "text": {
      "type": "text"
    },
    "twitter_language": {
      "type": "categorical"
    },
    "is_bot": {
      "type": "boolean"
    },
    "fluent_in_eng": {
      "type": "boolean"
    },
    "fluent_in_ukr": {
      "type": "boolean"
    },
    "fluent_in_rus": {
      "type": "boolean"
    },
    "fluent_in_und": {
      "type": "boolean"
    }
  },
  "relationships": {
    "written_by": {
      "source_entity_type": "tweet",
      "target_entity_type": "user"
    },
    "followed_by": {
      "source_entity_type": "user",
      "target_entity_type": "user"
    }
  }
}
