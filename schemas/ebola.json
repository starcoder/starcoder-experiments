{
  "@context": {
    "@vocab": "http://schema.org"
  },
  "meta": {
    "name": "Ebola Misinformation",
    "owners": [
      "Tara Sell"
    ],
    "description": "Tweets related to the 2014 Ebola outbreak.",
    "domain": "Public Health",
    "id_property": "id",
    "entity_type_property": "entity_type"
  },
  "entity_types": {
    "user": {
      "meta": {
        "display": "{0.name}"
      },
      "properties": [
        "name",
        "screen_name"
      ]
    },
    "tweet": {
      "meta": {
        "display": "{0.lang} {0.posted_at}"
      },
      "properties": [
        "text",
        "created_at",
        "retweet_count",
        "favorite_count"
      ]
    }
  },
  "properties": {
    "name": {
      "type": "text"
    },
    "screen_name": {
      "type": "text"
    },
    "retweet_count": {
      "type": "scalar"
    },
    "favorite_count": {
      "type": "scalar"
    },
    "created_at": {
      "type": "datetime"
    },
    "text": {
      "type": "text"
    }
  },
  "relationships": {
    "tweeted_by": {
      "source_entity_type": "tweet",
      "target_entity_type": "user"
    }
  }
}
