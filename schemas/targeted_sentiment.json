{
  "@context": {
    "@vocab": "http://schema.org"
  },
  "meta": {
    "name": "Targeted Sentiment Analysis",
    "owners": [],
    "description": "Parse trees of IMDB reviews with nodes annotated according to sentiment.",
    "domain": "Computational Linguistics",
    "id_property": "id",
    "entity_type_property": "entity_type"
  },
  "entity_types": {
    "node": {
      "meta": {
        "display": "{0.sentiment}"
      },
      "properties": [
        "sentiment",
        "split"
      ]
    },
    "word": {
      "meta": {
        "display": "{0.text}"
      },
      "properties": [
        "text",
        "split"
      ]
    }
  },
  "properties": {
    "sentiment": {
      "type": "scalar"
    },
    "text": {
      "type": "text"
    },
    "split": {
      "type": "categorical"
    }
  },
  "relationships": {
    "child_of": {
      "source_entity_type": "node",
      "target_entity_type": "node"
    },
    "text_of": {
      "source_entity_type": "word",
      "target_entity_type": "node"
    }
  }
}