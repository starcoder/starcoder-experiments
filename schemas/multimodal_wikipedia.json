{
  "@context": {
    "@vocab": "http://schema.org"
  },
  "meta": {
    "name": "Multimodal Wikipedia",
    "owners": [],
    "description": "Featured Wikipedia articles with associated image, audio, and video content.",
    "domain": "Other",
    "id_property": "id",
    "entity_type_property": "entity_type"
  },
  "entity_types": {
    "article": {
      "meta": {
        "display": "{0.title}"
      },
      "properties": [
        "title",
        "text"
      ]
    },
    "video": {
      "properties": [
        "audio",
        "video"
      ]
    },
    "audio": {
      "properties": [
        "audio"
      ]
    },
    "image": {
      "properties": [
        "image"
      ]
    }
  },
  "properties": {
    "title": {
      "type": "categorical"
    },
    "text": {
      "type": "text"
    },
    "video": {
      "type": "video",
      "width": 128,
      "height": 128,
      "channels": 3,
      "channel_size": 24
    },
    "audio": {
      "type": "audio",
      "channels": 1,
      "channel_size": 8
    },
    "image": {
      "type": "image",
      "width": 128,
      "height": 128,
      "channels": 3,
      "channel_size": 8
    }
  },
  "relationships": {
    "links_to_article": {
      "source_entity_type": "article",
      "target_entity_type": "article"
    },
    "has_audio": {
      "source_entity_type": "article",
      "target_entity_type": "audio"
    },
    "has_video": {
      "source_entity_type": "article",
      "target_entity_type": "video"
    },
    "has_image": {
      "source_entity_type": "article",
      "target_entity_type": "image"
    }
  }
}