{
  "@context": {
    "@vocab": "http://schema.org"
  },
  "meta": {
    "name": "Paris Tax Rolls",
    "owners": [
      "Nathan Daniels"
    ],
    "description": "Tax records from Medieval Paris.",
    "domain": "History",
    "id_property": "id",
    "entity_type_property": "entity_type"
  },
  "entity_types": {
    "tax_roll": {
      "meta": {
        "display": "{0.year}"
      },
      "properties": [
        "year"
      ]
    },
    "parish": {
      "meta": {
        "display": "{0.parish_name}"
      },
      "properties": [
        "parish_name"
      ]
    },
    "street": {
      "meta": {
        "display": "{0.street_name}"
      },
      "properties": [
        "street_name",
        "street_coordinates"
      ]
    },
    "role": {
      "meta": {
        "display": "{0.role_name}"
      },
      "properties": [
        "role_type",
        "role_name"
      ]
    },
    "person": {
      "meta": {
        "display": "{0.person_name}"
      },
      "properties": [
        "person_name"
      ]
    },
    "debt": {
      "meta": {
        "display": "{0.amount} {0.currency}"
      },
      "properties": [
        "amount",
        "currency",
        "status"
      ]
    }
  },
  "properties": {
    "year": {
      "type": "date",
      "format": "%Y"
    },
    "amount": {
      "type": "scalar"
    },
    "parish_name": {
      "type": "categorical"
    },
    "street_name": {
      "type": "categorical"
    },
    "street_coordinates": {
      "type": "place"
    },
    "role_name": {
      "type": "categorical"
    },
    "role_type": {
      "type": "categorical"
    },
    "person_name": {
      "type": "text"
    },
    "currency": {
      "type": "categorical"
    },
    "status": {
      "type": "categorical"
    }
  },
  "relationships": {
    "recorded_in": {
      "source_entity_type": "debt",
      "target_entity_type": "tax_roll"
    },
    "owed_by": {
      "source_entity_type": "debt",
      "target_entity_type": "person"
    },
    "lives_on": {
      "source_entity_type": "person",
      "target_entity_type": "street"
    },
    "part_of": {
      "source_entity_type": "street",
      "target_entity_type": "parish"
    },
    "acts_as": {
      "source_entity_type": "person",
      "target_entity_type": "role"
    }
  }
}