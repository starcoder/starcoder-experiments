{
  "@context": {
    "@vocab": "http://schema.org"
  },
  "meta": {
    "name": "Smoking and Vaping",
    "owners": [
      "Johannes Thrul"
    ],
    "description": "Differential study of how tobacco users approach and describe their attempts to quit.",
    "domain": "Public Health",
    "id_property": "id",
    "entity_type_property": "entity_type"
  },
  "entity_types": {
    "comment": {
      "meta": {
        "display": "{0.id}"
      },
      "properties": [
        "gilded",
        "text",
        "creation_time",
        "score"
      ]
    },
    "submission": {
      "meta": {
        "display": "{0.title}"
      },
      "properties": [
        "title",
        "text",
        "creation_time",
        "gilded",
        "score"
      ]
    },
    "user": {
      "meta": {
        "display": "{0.user_name}"
      },
      "properties": [
        "user_name"
      ]
    },
    "subreddit": {
      "meta": {
        "display": "{0.subreddit_name}"
      },
      "properties": [
        "subreddit_name"
      ]
    }
  },
  "properties": {
    "subreddit_name": {
      "type": "categorical"
    },
    "user_name": {
      "type": "categorical"
    },
    "score": {
      "type": "scalar"
    },
    "gilded": {
      "type": "scalar"
    },
    "text": {
      "type": "text"
    },
    "title": {
      "type": "text"
    },
    "creation_time": {
      "type": "datetime",
      "format": "utc"
    }
  },
  "relationships": {
    "written_by": {
      "source_entity_type": "comment",
      "target_entity_type": "user"
    },
    "submitted_by": {
      "source_entity_type": "submission",
      "target_entity_type": "user"
    },
    "response_to": {
      "source_entity_type": "comment",
      "target_entity_type": "comment"
    },
    "posted_in": {
      "source_entity_type": "submission",
      "target_entity_type": "subreddit"
    },
    "for_submission": {
      "source_entity_type": "comment",
      "target_entity_type": "submission"
    }
  }
}