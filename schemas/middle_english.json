{
  "@context": {
    "@vocab": "http://schema.org"
  },
  "meta": {
    "name": "Middle English Scansion",
    "owners": [
      "Chris Cannon"
    ],
    "description": "The Canterbury Tales with scansion and grammatical information.",
    "domain": "Literature",
    "id_property": "id",
    "entity_type_property": "entity_type"
  },
  "entity_types": {
    "book": {
      "meta": {
        "display": "{0.book_name}"
      },
      "properties": [
        "book_name"
      ]
    },
    "section": {
      "meta": {
        "display": "{0.section_name}"
      },
      "properties": [
        "section_name"
      ]
    },
    "line": {
      "meta": {
        "display": "{0.line_name}"
      },
      "properties": [
        "line_name"
      ]
    },
    "word": {
      "meta": {
        "display": "{0.token}"
      },
      "properties": [
        "token",
        "lemma",
        "tagging",
        "stress"
      ]
    }
  },
  "properties": {
    "book_name": {
      "type": "categorical"
    },
    "section_name": {
      "type": "categorical"
    },
    "line_name": {
      "type": "text"
    },
    "token": {
      "type": "text"
    },
    "lemma": {
      "type": "text"
    },
    "tagging": {
      "type": "categorical"
    },
    "stress": {
      "type": "categorical"
    }
  },
  "relationships": {
    "word_from": {
      "source_entity_type": "word",
      "target_entity_type": "line"
    },
    "line_from": {
      "source_entity_type": "line",
      "target_entity_type": "section"
    },
    "section_from": {
      "source_entity_type": "section",
      "target_entity_type": "book"
    }
  }
}
