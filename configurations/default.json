[
    [
	"$",
	{
	    "proportions" : [
		0.8,
		0.1,
		0.1
	    ],
	    "splitter" : "starcoder.splitter.SampleComponents",
	    "batchifier" : "starcoder.batchifier.SampleComponents",
	    "scheduler" : "starcoder.scheduler.BasicScheduler",
	    "projector" : "starcoder.projector.MLPProjector",
	    "autoencoder" : "starcoder.autoencoder.BasicAutoencoder",
	    "null_autoencoder" : "starcoder.autoencoder.NullAutoencoder",
	    "depth" : 1,
	    "reverse_relationships" : true,
	    "shared_entity_types" : [],
	    "batch_size" : 4096,
	    "max_epochs" : 2,
	    "patience" : 5,
	    "early_stop" : 10
