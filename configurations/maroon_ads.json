[
    [
	"$",
	{
	    "shared_entity_types" : ["city"],
	    "depth" : 2
	}
    ],    
    [
	"$.properties[?(@.type=='date')]",
	{
	    "format" : ["%m/%d/%Y", "%d-%b-%Y", "%d-%B-%Y", "%b-%Y", "%Y", "%d-%B%Y", "%d-%b%Y"]
	}
    ],
    [
	"$.properties[?(@.type=='text')]",
	{
	    "decoder" : "starcoder.property_decoder.SequenceDecoder",
	    "loss" : "starcoder.property_loss.SequenceLoss"
	}
    ],
    [
	"$.relationships",
	{
	    "source_summarizer" : "starcoder.summarizer.DANSummarizer",
	    "target_summarizer" : "starcoder.summarizer.DANSummarizer",
	    "relationship_detector" : "starcoder.relationship_detector.MLP",
	    "dropout_probability" : 0.2
	}
    ]
]
